mixin breadcrumb(title, image, linkBack)
  section.bg-cover.bg-center.bg-no-repeat.relative(
    class='pt-[22%] min-h-[200px]',
    style='background-image: linear-gradient(180deg, rgba(98,97,102,0.5) 0%, rgba(194,194,204,0.25) 100%), url(assets/images/' + image + ')'
  )
    h2.c-title-breadcrumb= title
